{
  "name": "dlopezcastellote.dev",
  "private": true,
  "version": "0.0.4",
  "description": "David López Castellote Personal Website",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "deploy": "./deploy-site-to-master.sh",
    "blog:dev": "node scripts/parse-blog.js",
    "blog": "cross-env NODE_ENV=PRODUCTION node scripts/parse-blog.js",
    "build:deploy": "npm run blog && npm run deploy",
    "scripts:test": "jest --verbose ./scripts/tests"
  },
  "dependencies": {
    "dlc-css-terminal": "0.0.6",
    "dlc-seo-tags": "0.0.73",
    "ionicons": "5.0.1",
    "markdown-toc": "^1.2.0"
  },
  "devDependencies": {
    "@stencil/core": "^1.14.0",
    "@stencil/helmet": "^0.3.2",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.1",
    "axios": "^0.19.2",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "estimated-reading-time": "0.0.1",
    "fs-extra": "^9.0.0",
    "gray-matter": "^4.0.1",
    "highlight.js": "^9.18.1",
    "html-entities": "^1.3.1",
    "jest": "^26.0.1",
    "prismjs": "^1.20.0",
    "remarkable": "^1.7.1",
    "workbox-build": "4.3.1",
    "cross-env": "latest"
  },
